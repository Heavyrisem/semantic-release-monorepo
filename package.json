{
  "name": "semantic-release-monorepo",
  "version": "7.0.5",
  "description": "Plugins for `semantic-release` allowing it to be used with a monorepo.",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "files": [
    "dist"
  ],
  "workspaces": [
    "test-workspace"
  ],
  "exports": {
    ".": {
      "node": "./dist/index.js",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "format": "prettier --write --single-quote --trailing-comma es5",
    "format:all": "yarn format \"./**/*.js\"",
    "test": "vitest --no-threads",
    "build": "tsup"
  },
  "license": "MIT",
  "peerDependencies": {
    "semantic-release": ">=22"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^11.1.0",
    "@semantic-release/exec": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^9.2.1",
    "@semantic-release/release-notes-generator": "^12.1.0",
    "@types/git-log-parser": "^1.2.2",
    "@types/ramda": "0.27.66",
    "debug": "4.3.4",
    "execa": "8.0.1",
    "file-url": "4.0.0",
    "fs-extra": "11.1.1",
    "get-stream": "6.0.1",
    "git-log-parser": "1.2.0",
    "husky": "4.2.1",
    "lint-staged": "10.0.7",
    "p-each-series": "3.0.0",
    "p-limit": "4.0.0",
    "pkg-up": "4.0.0",
    "prettier": "1.19.1",
    "ramda": "0.27.2",
    "read-pkg": "8.1.0",
    "semantic-release": "22.0.6",
    "tempy": "1.0.1",
    "tsup": "^7.2.0",
    "vitest": "0.34.6"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "yarn format"
    ]
  },
  "packageManager": "yarn@4.0.1"
}
